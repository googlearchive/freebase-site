{
  "ttl": 0,
  "error_page" : "error/error.mjt",
  "csrf_protection": "strong",
  "handlers": {
    "mjt": "handlers/mjt_handler.sjs",
    "css": "handlers/css_handler.sjs",
    "css_manifest": "handlers/css_manifest_handler.sjs",
    "js_manifest": "handlers/js_manifest_handler.sjs",
    "tagged_static": "handlers/tagged_static_handler.sjs",
    "tagged_binary": "handlers/tagged_binary_handler.sjs", 
    "freebase_template": "handlers/template_handler.sjs",
    "controller": "handlers/controller_handler.sjs",
    "ajax": "handlers/ajax_handler.sjs",
    "coffee_script": "handlers/coffee_script_handler.sjs"
  },
  "extensions": {
    "ico" : { 
      "handler": "tagged_binary",
      "media_type" : "image/x-icon"
    },
    "css": {
      "handler": "css",
      "media_type": "text/css"
    },
    "less": {
      "handler": "css",
      "media_type": "text/css"
    },
    "mf.css": {
      "handler": "css_manifest",
      "media_type": "text/css"
    },
    "mf.js": {
      "handler": "js_manifest",
      "media_type": "text/javascript"
    },
    "template": {
      "handler": "freebase_template"
    },
    "controller": {
      "handler": "controller"
    },
    "ajax": {
      "handler": "ajax"
    },
    "omf.js": {
      "handler": "js_manifest",
      "media_type": "text/javascript"
    }, 
    "omf.css": {
      "handler": "css_manifest",
      "media_type": "text/css"
    },
    "properties": {
      "handler": "acre_script"
    },
    "cs": {
      "handler": "coffee_script"
    },
    "tab": {
      "handler": "acre_script"
    }      
  },
  "libs" : {
    "suggest" : {
      "base_url": "http://freebaselibs.com/static/suggest/",
      "version": "1.5"
    },
    "jquery" : {
      "base_url": "http://ajax.googleapis.com/ajax/libs/jquery/",
      "version": "1.5.2"
    },
    "dojo" : {
      "base_url": "https://ajax.googleapis.com/ajax/libs/dojo/",
      "version": "1.6.0"
    }
  }
}
